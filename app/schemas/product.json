{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "stock": {
      "type": "number"
    },
    "type": {
        "enum": ["DARK", "CLEAN", "AMBER"]
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "description",
    "image",
    "name",
    "price",
    "stock",
    "type"
  ]
}
